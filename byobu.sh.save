#!/bin/bash

# Mulai byobu tanpa attach session lama
byobu new-session -d

# Pane pertama: masuk ke folder
byobu send-keys "cd ~/WTP/MiniPlant" Enter

# Split vertikal
byobu split-window -h

# Pane kedua: masuk ke folder
byobu send-keys -t 1 "cd ~/WTP/MiniPlant" Enter
#!/bin/bash

# Mulai byobu tanpa attach session lama
byobu new-session -d

# Pane pertama: masuk ke folder
byobu send-keys "cd ~/WTP/MiniPlant" C-m

# Split vertikal
byobu split-window -v

# Pane kedua: masuk ke folder
byobu send-keys -t 1 "cd ~/WTP/MiniPlant" C-m

# Attach session
byobu attach


# Attach session
byobu attach
